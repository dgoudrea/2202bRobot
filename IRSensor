#include <IRremote.h>
#include <IRremoteInt.h>
#include <IRremoteTools.h>


int IR_Pin = 3;
IRrecv irrecv(IR_Pin);
decode_results results;

void setup() {
Serial.begin(2400);
irrecv.enableIRIn();

}

void loop() {
if(irrecv.decode(&results)){
  Serial.println(results.value,HEX);
  irrecv.resume();
}
}
